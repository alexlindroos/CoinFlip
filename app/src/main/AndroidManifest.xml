<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.codemate.testbot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".chat.ChatActivity"></activity>
        <activity android:name=".cv.YourCvActivity"></activity>
        <activity android:name=".login.EmployerLoginActivity"></activity>
        <activity android:name=".signup.EmployerSignupActivity"></activity>
        <activity android:name=".login.JobseekerLoginActivity"></activity>
        <activity android:name=".signup.JobseekerSignupActivity"></activity>
        <activity android:name=".cv.CVList">
        <meta-data
            android:name="android.app.searchable"
            android:resource="@xml/searchable">
        </meta-data>></activity>
        <activity android:name=".profile.ProfilePageActivity"></activity>
        <activity android:name=".onboarding.WelcomeOnBoarding">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".profile.MyProfileActivity"></activity>
    </application>

</manifest>